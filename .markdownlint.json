{
  "default": true,
  "line-length": {
    "line_length": 100,
    "code_blocks": false,
    "tables": false
  },
  "no-trailing-spaces": true,
  "no-hard-tabs": true,
  "no-duplicate-heading": {
    "siblings_only": true
  },
  "no-multiple-blanks": {
    "maximum": 1
  },
  "list-marker-space": true,
  "ul-style": {
    "style": "dash"
  },
  "no-inline-html": false,
  "hr-style": {
    "style": "---"
  },
  "no-trailing-punctuation": {
    "punctuation": ".,;:!"
  },
  "commands-show-output": false
}